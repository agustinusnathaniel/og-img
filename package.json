{
  "name": "nextarter-chakra",
  "version": "1.1.2",
  "private": true,
  "author": "sozonome",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "build:turbo": "npx turbo run build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm format",
    "lint:turbo": "npx turbo run lint",
    "type-check": "tsc --noEmit",
    "type-check:turbo": "npx turbo run type-check",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/outfit": "^5.0.8",
    "@vercel/og": "^0.5.20",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.4",
    "lodash-es": "^4.17.21",
    "next": "^13.5.6",
    "next-seo": "^6.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-icons": "^4.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@types/lodash-es": "^4.17.10",
    "@types/node": "20.8.7",
    "@types/react": "^18.2.31",
    "eslint": "^8.52.0",
    "eslint-config-next": "13.5.6",
    "eslint-config-sznm": "^2.0.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.0.3",
    "standard-version": "^9.5.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.x.x",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@8.1.0"
}
