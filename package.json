{
  "name": "nextarter-chakra",
  "version": "1.1.2",
  "private": true,
  "author": "sozonome",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "build:turbo": "npx turbo run build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm format",
    "lint:turbo": "npx turbo run lint",
    "type-check": "tsc --noEmit",
    "type-check:turbo": "npx turbo run type-check",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/outfit": "^5.0.13",
    "@vercel/og": "^0.6.2",
    "clsx": "^2.1.1",
    "framer-motion": "^11.1.9",
    "lodash-es": "^4.17.21",
    "next": "^14.2.3",
    "next-seo": "^6.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.4",
    "react-icons": "^5.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "20.12.11",
    "@types/react": "^18.3.2",
    "eslint": "^8.52.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-sznm": "^2.0.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.2.5",
    "standard-version": "^9.5.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.x.x",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.1.1"
}
